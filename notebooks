    "\n",
    "# Drop rows without a valid EPC date\n",
    "epc_issued = epc_issued.dropna(subset=[\"epc_valid_from\"])\n",
    "\n",
    "# Create a reference intervention date (median EPC issue date)\n",
    "intervention_date = epc_issued[\"epc_valid_from\"].median()\n",
    "\n",

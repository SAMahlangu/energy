    "import gradio as gr\n",
    "import random\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# -----------------------------\n",
    "# Core calculation function\n",
    "# -----------------------------\n",
    "def run_epc_analysis(\n",
    "    baseline_energy,\n",
    "    efficiency_improvement,\n",
    "    tariff,\n",
    "    emission_factor\n",
    "):\n",
    "    # Calculations\n",
    "    energy_saved = baseline_energy * (efficiency_improvement / 100)\n",
    "    cost_savings = energy_saved * tariff\n",
    "    co2_avoided = (energy_saved * emission_factor) / 1000  # kg â†’ tons\n",
    "\n",
    "    # -----------------------------\n",
    "    # Plot\n",
    "    # -----------------------------\n",
    "    fig, ax = plt.subplots()\n",
    "    metrics = [\"Energy Saved (kWh)\", \"COâ‚‚ Avoided (tons)\"]\n",
    "    values = [energy_saved, co2_avoided]\n",
    "\n",
    "    ax.bar(metrics, values)\n",
    "    ax.set_title(\"Scenario Impact Summary\")\n",
    "    ax.set_ylabel(\"Impact Value\")\n",
    "    plt.tight_layout()\n",
    "\n",
    "    # -----------------------------\n",
    "    # Interpretation text\n",
    "    # -----------------------------\n",
    "    interpretation = f\"\"\"\n",
    "ðŸ” **Scenario Interpretation**\n",
    "\n",
    "â€¢ Efficiency improvement: **{efficiency_improvement:.1f}%**  \n",
    "â€¢ Electricity tariff: **R{tariff:.2f}/kWh**  \n",
    "â€¢ COâ‚‚ emission factor: **{emission_factor:.2f} kg/kWh**\n",
    "\n",
    "ðŸ“Š **Impact Results**\n",
    "\n",
    "â€¢ Energy saved: **{energy_saved:,.0f} kWh**  \n",
    "â€¢ Cost savings: **R{cost_savings:,.0f}**  \n",
    "â€¢ COâ‚‚ avoided: **{co2_avoided:,.2f} tons**\n",
    "\n",
    "ðŸ’¡ **Insight**\n",
    "\n",
    "This scenario shows that improving EPC efficiency leads to **substantial energy and cost reductions**, while also delivering meaningful **carbon mitigation benefits**.  \n",
    "At scale, such improvements justify **stronger EPC enforcement, retrofit incentives, and energy efficiency policies**.\n",
    "\"\"\"\n",
    "\n",
    "    return fig, interpretation\n",
    "\n",
    "\n",
    "# -----------------------------\n",
    "# Random input generator\n",
    "# -----------------------------\n",
    "def randomize_inputs():\n",
    "    return (\n",
    "        random.randint(50_000_000, 500_000_000),  # baseline energy\n",
    "        random.uniform(5, 30),                    # efficiency %\n",
    "        random.uniform(1.8, 3.5),                 # tariff\n",
    "        random.uniform(0.6, 0.9)                  # emission factor\n",
    "    )\n",
    "\n",
    "\n",
    "# -----------------------------\n",
    "# Gradio Interface\n",
    "# -----------------------------\n",
    "with gr.Blocks(title=\"EPC Scenario Impact Analyzer\") as demo:\n",
    "    gr.Markdown(\"## ðŸ¢ EPC Scenario Impact Analyzer\")\n",
    "    gr.Markdown(\"Simulate how EPC efficiency improvements affect energy, cost, and emissions.\")\n",
    "\n",
    "    with gr.Row():\n",
    "        baseline_energy = gr.Number(\n",
    "            label=\"Baseline Energy Consumption (kWh)\",\n",
    "            value=300_000_000\n",
    "        )\n",
    "        efficiency = gr.Slider(\n",
    "            1, 40, value=10, step=0.5,\n",
    "            label=\"Efficiency Improvement (%)\"\n",
    "        )\n",
    "\n",
    "    with gr.Row():\n",
    "        tariff = gr.Number(\n",
    "            label=\"Electricity Tariff (R/kWh)\",\n",
    "            value=2.45\n",
    "        )\n",
    "        emission_factor = gr.Number(\n",
    "            label=\"COâ‚‚ Emission Factor (kg/kWh)\",\n",
    "            value=0.71\n",
    "        )\n",
    "\n",
    "    with gr.Row():\n",
    "        run_btn = gr.Button(\"â–¶ Run Analysis\")\n",
    "        random_btn = gr.Button(\"ðŸŽ² Randomize Inputs\")\n",
    "\n",
    "    plot_output = gr.Plot(label=\"Impact Visualization\")\n",
    "    text_output = gr.Markdown()\n",
    "\n",
    "    run_btn.click(\n",
    "        run_epc_analysis,\n",
    "        inputs=[baseline_energy, efficiency, tariff, emission_factor],\n",
    "        outputs=[plot_output, text_output]\n",
    "    )\n",
    "\n",
    "    random_btn.click(\n",
    "        randomize_inputs,\n",
    "        outputs=[baseline_energy, efficiency, tariff, emission_factor]\n",
    "    )\n",
    "\n",
    "# -----------------------------\n",
    "# Launch Interface\n",
    "# -----------------------------\n",

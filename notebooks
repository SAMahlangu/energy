    "#Baseline Energy Modeling\n",
    "#Prepare Features\n",
    "exclude_cols = [\"epc_issue_date\", target_col]\n",
    "features = epc_issued.drop(columns=[c for c in exclude_cols if c in epc_issued.columns])\n",
    "\n",
    "X = features.select_dtypes(include=np.number).fillna(0)\n",
    "y = epc_issued[target_col]\n",

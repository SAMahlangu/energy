   "cell_type": "code",
   "execution_count": 57,
   "id": "fb969dcb-79e4-4b16-9f5a-738499f568da",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 900x500 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# ============================================================\n",
    "# CELL 14: Scenario Simulations\n",
    "# ============================================================\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "# -------------------------------\n",
    "# Work from counterfactual dataset\n",
    "# -------------------------------\n",
    "df = epc_cf.copy()\n",
    "\n",
    "# -------------------------------\n",
    "# Ensure EPC date exists and is datetime\n",
    "# -------------------------------\n",
    "df[\"epc_valid_from\"] = pd.to_datetime(\n",
    "    df[\"epc_valid_from\"], errors=\"coerce\"\n",
    ")\n",
    "\n",
    "# Drop rows without valid dates\n",
    "df = df.dropna(subset=[\"epc_valid_from\"])\n",
    "\n",
    "# Extract year\n",
    "df[\"year\"] = df[\"epc_valid_from\"].dt.year\n",
    "\n",
    "# -------------------------------\n",
    "# Baseline vs Observed energy\n",
    "# -------------------------------\n",
    "yearly_actual = (\n",
    "    df.groupby(\"year\")[target_col]\n",
    "    .mean()\n",
    "    .reset_index(name=\"actual_energy\")\n",
    ")\n",
    "\n",
    "yearly_baseline = (\n",
    "    df.groupby(\"year\")[\"baseline_energy_no_epc\"]\n",
    "    .mean()\n",
    "    .reset_index(name=\"baseline_energy\")\n",
    ")\n",
    "\n",
    "yearly = yearly_actual.merge(\n",
    "    yearly_baseline, on=\"year\", how=\"inner\"\n",
    ")\n",
    "\n",
    "# -------------------------------\n",
    "# Scenario: 20% stronger EPC effect\n",
    "# -------------------------------\n",
    "yearly[\"scenario_20pct_efficiency\"] = (\n",
    "    yearly[\"baseline_energy\"]\n",
    "    - 1.2 * (yearly[\"baseline_energy\"] - yearly[\"actual_energy\"])\n",
    ")\n",
    "\n",
    "# -------------------------------\n",
    "# Plot results\n",
    "# -------------------------------\n",
    "plt.figure(figsize=(9,5))\n",
    "\n",
    "plt.plot(\n",

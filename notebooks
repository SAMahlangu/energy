   "source": [
    "# Constants\n",
    "COST_PER_KWH = 2.5     # ZAR per kWh\n",
    "CO2_FACTOR = 0.9       # kg CO2 per kWh\n",
    "\n",
    "# Step 1: Calculate benchmark in total kWh for the building\n",
    "# Assuming 'benchmark' is in kWh/mÂ²/year and 'epc_floor_area' is in mÂ²\n",
    "epc_issued[\"benchmark_total_kwh\"] = epc_issued[\"benchmark\"] * epc_issued[\"epc_floor_area\"]\n",
    "\n",
    "# Step 2: Calculate energy savings\n",
    "# Positive â†’ building used less energy than benchmark (good)\n",
    "# Negative â†’ building used more energy than benchmark (needs improvement)\n",
    "epc_issued[\"energy_savings\"] = epc_issued[\"benchmark_total_kwh\"] - epc_issued[\"total_energy_consumption\"]\n",
    "\n",
    "# Step 3: Calculate cost savings and CO2 saved\n",
    "epc_issued[\"cost_savings\"] = epc_issued[\"energy_savings\"] * COST_PER_KWH\n",
    "epc_issued[\"co2_saved_kg\"] = epc_issued[\"energy_savings\"] * CO2_FACTOR\n",
    "\n",
    "# Step 4: Quick summary\n",
    "summary = epc_issued[[\"energy_savings\", \"cost_savings\", \"co2_saved_kg\"]].sum()\n",
    "print(summary)\n"
   ]

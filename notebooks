   "source": [
    "# ============================================================\n",
    "# CELL 13: Cost & Carbon Impact Analysis\n",
    "# ============================================================\n",
    "\n",
    "# -------------------------------\n",
    "# Use counterfactual dataframe\n",
    "# -------------------------------\n",
    "df_impact = epc_cf.copy()\n",
    "\n",
    "# -------------------------------\n",
    "# Keep only positive savings\n",
    "# -------------------------------\n",
    "df_impact = df_impact[df_impact[\"energy_savings\"] > 0]\n",
    "\n",
    "# -------------------------------\n",
    "# Aggregate total energy savings\n",
    "# -------------------------------\n",
    "total_energy_savings_kwh = df_impact[\"energy_savings\"].sum()\n",
    "\n",
    "# -------------------------------\n",
    "# Assumptions (can be adjusted)\n",
    "# -------------------------------\n",
    "electricity_tariff_per_kwh = 2.50   # ZAR / kWh (South Africa avg)\n",
    "co2_factor = 0.90                   # kg CO2 / kWh (coal-heavy grid)\n",
    "\n",
    "# -------------------------------\n",
    "# Cost savings\n",
    "# -------------------------------\n",
    "total_cost_savings_zar = (\n",
    "    total_energy_savings_kwh * electricity_tariff_per_kwh\n",
    ")\n",
    "\n",
    "# -------------------------------\n",
    "# Carbon savings\n",
    "# -------------------------------\n",
    "total_co2_saved_kg = (\n",
    "    total_energy_savings_kwh * co2_factor\n",
    ")\n",
    "\n",
    "total_co2_saved_tons = total_co2_saved_kg / 1_000\n",
    "\n",
    "# -------------------------------\n",
    "# Results summary\n",
    "# -------------------------------\n",
    "impact_summary = {\n",
    "    \"Total Energy Saved (kWh)\": round(total_energy_savings_kwh, 2),\n",
    "    \"Total Cost Savings (ZAR)\": round(total_cost_savings_zar, 2),\n",
    "    \"CO2 Avoided (tons)\": round(total_co2_saved_tons, 2)\n",
    "}\n",
    "\n",
    "impact_summary\n"
   ]
  },

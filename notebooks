   "source": [
    "# ============================================================\n",
    "# CELL 12: Counterfactual Forecasting (No-EPC Scenario)\n",
    "# ============================================================\n",
    "\n",
    "# Create a working copy\n",
    "epc_cf = epc_issued.copy()\n",
    "\n",
    "# -------------------------------\n",
    "# Rebuild feature matrix EXACTLY as in Cell 11\n",
    "# -------------------------------\n",
    "X_full = epc_cf[feature_cols].copy()\n",
    "X_full = X_full.apply(pd.to_numeric, errors=\"coerce\")\n",
    "\n",
    "# -------------------------------\n",
    "# Keep only rows the model can predict on\n",
    "# -------------------------------\n",
    "valid_idx = X_full.dropna().index\n",
    "X_valid = X_full.loc[valid_idx]\n",
    "\n",
    "# -------------------------------\n",
    "# Counterfactual assumption:\n",
    "# EPC never happened â†’ post_epc = 0\n",
    "# -------------------------------\n",
    "X_cf = X_valid.copy()\n",
    "X_cf[\"post_epc\"] = 0\n",
    "\n",
    "# -------------------------------\n",
    "# Predict baseline (no EPC) energy\n",
    "# -------------------------------\n",
    "baseline_preds = rf.predict(X_cf)\n",
    "\n",
    "# -------------------------------\n",
    "# Assign predictions BACK using index alignment\n",
    "# -------------------------------\n",
    "epc_cf.loc[valid_idx, \"baseline_energy_no_epc\"] = baseline_preds\n",
    "\n",
    "# -------------------------------\n",
    "# Compute actual energy savings\n",
    "# -------------------------------\n",
    "epc_cf[\"energy_savings\"] = (\n",
    "    epc_cf[\"baseline_energy_no_epc\"] - epc_cf[target_col]\n",
    ")\n",
    "\n",
    "# -------------------------------\n",
    "# Inspect savings\n",
    "# -------------------------------\n",
    "epc_cf[\"energy_savings\"].describe()\n"
   ]
  },

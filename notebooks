    "#Standardize Column Names\n",
    "def clean_columns(df):\n",
    "    df.columns = (\n",
    "        df.columns\n",
    "        .str.strip()\n",
    "        .str.lower()\n",
    "        .str.replace(\" \", \"_\")\n",
    "    )\n",
    "    return df\n",
    "\n",
    "epc_issued = clean_columns(epc_issued)\n",
    "buildings = clean_columns(buildings)\n",
    "\n",
    "epc_issued.head()\n"
   ]
